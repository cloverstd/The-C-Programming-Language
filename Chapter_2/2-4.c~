/* Write an alternate version of squeeze(s1,s2) that deletes each character in the string s1 that
 * matches any character in the string s2 .
 * */

#include<stdio.h>
void squeeze(char s1[], char s2[]);

int main()
{
	char s2[] = "qwerty";
	char s1[] = "qawsedrftgsdgfsd";
	squeeze(s1, s2);
	printf("%s\n", s1);
	return 0;
}
/*
void squeeze(char s1[], char s2[])
{
	int i, j, k;
	for (i = k = 0; s1[i] != '\0'; i++)
	{
		for (j = 0; s2[j] != '\0' && s2[j] != s1[i]; j++)
			;
		if (s2[j] == '0')
			s1[k++] = s1[i];
	}
	s1[k] = '\0';
}
*/
void squeeze(char s1[], char s2[])
{
  int i, j, k;
  int instr2 = 0;

  for(i = j = 0; s1[i] != '\0'; i++)
  {
    instr2 = 0;
    for(k = 0; s2[k] != '\0' && !instr2; k++)
    {
      if(s2[k] == s1[i])
      {
        instr2 = 1;
      }
    } 

    if(!instr2)
    {
      s1[j++] = s1[i];
    } 
  }
  s1[j] = '\0';
}
